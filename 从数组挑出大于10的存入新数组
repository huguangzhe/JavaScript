<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <script>
        // 从数组中选中大于10的元素存入新数组
        // 1. 方法1 
        var arr = [2,0,6,1,77,0,52,0,25,7];
        var arr1 =[];
        var j = 0;
        for (var i = 0;i < arr.length;i++) {
            if (arr[i] >= 10) {
                //新数组应该从0开始 一次递增
                arr1[j] = arr[i];
                j++;
            }
        }
        console.log(arr1);
        //2. 方法二
        var arr = [2,0,6,1,77,0,52,0,25,7];
        var arr2 =[];
        for (var i = 0;i < arr.length;i++) {
            if (arr[i] >= 10) {
                //新数组应该从0开始 一次递增
                arr2[arr2.length] = arr[i];
            
            }
        }
        console.log(arr2);
    </script>
</head>
<body>
    
</body>
</html>
