    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <script>
        //1. 新增数组元素   修改length长度
        var arr = ['red' ,'green','blue'];
        arr.length = 5;//修改数组长度为5
        console.log(arr);
        //2. 新增数组元素   修改索引号
        var arr1 = ['red','blue','green'];
        arr1[3]= 'pink';
        console.log(arr1);
        arr1[4] = 'pink2';
        console.log(arr1);
        arr1[0] = 'yellow'; //这里是替换原来的数组元素
        arr1 = '有点意思';  //不要直接给数组赋值  否则里面的数组元素都没有了 
    </script>
</head>
