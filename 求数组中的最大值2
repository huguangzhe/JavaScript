    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <script>
        function numMax(arr) {
            var max = arr[0];
            var arr = [5,2,99,101,67,77];
            for (var i = 1;i < arr.length;i++) {
                if(arr[i]>max) {
                    max = arr[i];
                }
            }
            return max;
        }
        var re=numMax([5,2,99,101,67,77]);
        console.log(re);
    </script>
</head>
